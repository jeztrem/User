      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute name="Span" type="xs:unsignedByte" use="optional" />
          <xs:attribute name="TreeLevel" type="xs:unsignedByte" use="optional" />
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
</xs:schema>
