      <xs:sequence>
        <xs:element ref="Replace" minOccurs="0" maxOccurs="unbounded" />
      </xs:sequence>
      <xs:attribute name="Key" type="xs:string" use="required" />
      <xs:attribute name="ComplaintLevel" type="xs:string" use="required" />
      <xs:attribute ref="Side" use="optional" />
